@model URUNTAKIPMVC.Models.MultiView
@{
    ViewBag.Title = "urungecmis";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="portlet light bordered">
    <div class="portlet-title">
        <div class="caption font-dark">
            <i class="icon-settings font-dark"></i>
            <span class="caption-subject bold uppercase">Basic</span>
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body">
        <div id="sample_1_wrapper" class="dataTables_wrapper no-footer">
            <table class="table table-striped table-bordered table-hover dt-responsive dataTable no-footer dtr-inline collapsed" width="100%" id="sample_1" role="grid" aria-describedby="sample_1_info" style="width: 100%;">
                <thead>
                    <tr>
                        <th>Marka</th>
                        <th>Model</th>
                        <th>Notu</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.UrunGecmisList)
                    {
                        <tr>
                            <td>@item.Urunler.Marka</td>
                            <td>@item.Urunler.Model</td>
                            <td>@item.Notu</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

</div>

